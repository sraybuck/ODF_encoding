  <text>
    <body>
      <schemaSpec ident="antarctic_test" start="TEI teiCorpus" source="teipublisher.odd">
        <moduleRef key="analysis" include="interp interpGrp span"/>
        <moduleRef key="core" except="binaryObject divGen headItem headLabel"/>
        <moduleRef key="corpus" include="particDesc"/>
        <moduleRef key="figures" except="formula notatedMusic"/>
        <moduleRef key="gaiji" include="g"/>
        <moduleRef key="header"/>
        <moduleRef key="linking" include="ab anchor link seg"/>
        <moduleRef key="tagdocs" include="att code gi ident"/>
        <moduleRef key="tei"/>
        <moduleRef key="textstructure" except="div1 div2 div3 div4 div5 div6 div7"/>
        <moduleRef key="transcr" include="fw"/>
        <moduleRef key="namesdates" include="listRelation listPlace listPerson persName placeName orgName roleName personGrp geo affiliation age birth death education nationality location relation occupation person place org state"/>
        <elementSpec ident="ab" module="core" mode="change">
            <modelGrp>
            <model behaviour="block" predicate=".[@type eq 'section-head']">
                <outputRendition>
                    font-family: "Courier New", Courier, monospace;
                    text-align: center;
                    font-size: 1.2 em;
                    text-decoration: underline;
                    padding-bottom: 10px;
                </outputRendition>
            </model>
            <model behaviour="block">
                <outputRendition>
                    font-family: "Courier New", Courier, monospace;
                    margin-bottom: 10px;
                    clear: both;
                </outputRendition>
            </model>
            </modelGrp>
        </elementSpec>
        <elementSpec ident="bibl" module="core" mode="change">
            <model behaviour="block"/>
        </elementSpec>
        <elementSpec ident="choice" mode="change">
            <model predicate="sic and corr" behaviour="alternate">
                <param name="default" value="corr"/>
                <param name="alternate" value="sic"/>
            </model>
        </elementSpec>
        <elementSpec ident="date" module="namesdates" mode="change">
            <model behaviour="inline" predicate=".[ancestor::div[@type eq 'nameplate']]">
                <outputRendition>
                    float: right;
                    display: inline-block;
                </outputRendition>
            </model>
        </elementSpec>
        <elementSpec ident="div" module="textstructure" mode="change">
          <attList>
            <attDef ident="type" mode="change" usage="req"/>
          </attList>
            <model behaviour="block" predicate=".[@type eq 'article']">
                <outputRendition>
                    padding-bottom: 15px;
                </outputRendition>
            </model>
        </elementSpec>
        <elementSpec ident="fw" module="transcr" mode="change">
          <modelGrp>
            <model behaviour="block" predicate=".[@type eq 'header']">
            <outputRendition>
                text-align: left;
                text-decoration: underline;
                font-family: "Courier New", Courier, monospace;
                padding-bottom: 10px;
            </outputRendition>
          </model>
          <model behaviour="block">
            <outputRendition>
                text-align: center;
                font-family: "Courier New", Courier, monospace;
            </outputRendition>
          </model>
          </modelGrp>
        </elementSpec>
        <elementSpec ident="head" module="core" mode="change">
            <model behaviour="block">
                <outputRendition>
                    font-family: "Courier New", Courier, monospace;
                    font-size: 1.7em;
                    padding-bottom: 10px;
                    text-align: center;
                </outputRendition>
             </model>
        </elementSpec>
        <elementSpec ident="lb" module="core" mode="change">
            <model behaviour="omit"/>
        </elementSpec>
        <elementSpec ident="num" module="core" mode="change">
            <model behaviour="inline" predicate=".[ancestor::div[@type eq 'nameplate']]">
                <outputRendition>
                    float: left;
                    display: inline-block;
                    padding-right: 15px;
                </outputRendition>
            </model>
        </elementSpec>
        <elementSpec ident="orgName" module="namesdates" mode="change">
          <attList>
            <attDef ident="ref" mode="change" usage="req"/>
          </attList>
        </elementSpec>
        <elementSpec ident="p" module="core" mode="change">
          <model behaviour="block">
            <outputRendition>
                font-family: "Courier New", Courier, monospace;
                margin-bottom: 10px;
                clear: both;
            </outputRendition>
          </model>
        </elementSpec>
        <elementSpec ident="pb" module="core" mode="change">
          <attList>
            <attDef ident="facs" mode="change" usage="req"/>
            <attDef ident="n" mode="change" usage="req"/>
          </attList>
          <model behaviour="omit"/>
        </elementSpec>
        <elementSpec ident="placeName" module="namesdates" mode="change">
          <attList>
            <attDef ident="ref" mode="change" usage="req"/>
          </attList>
        </elementSpec>
        <elementSpec ident="persName" module="namesdates" mode="add">
          <attList>
            <attDef ident="ref" mode="change" usage="req"/>
          </attList>
        </elementSpec>
        <elementSpec ident="person" module="namesdates" mode="change">
          <attList>
            <attDef ident="xml:id" mode="change" usage="req"/>
          </attList>
        </elementSpec>
        <elementSpec ident="place" module="namesdates" mode="change">
          <attList>
            <attDef ident="xml:id" mode="change" usage="req"/>
          </attList>
        </elementSpec>
        <elementSpec ident="rs" mode="change" module="core">
          <attList>
            <attDef ident="ref" mode="change" usage="req"/>
            <attDef ident="type" mode="replace" usage="req">
              <datatype>
                <dataRef key="teidata.enumerated"/>
              </datatype>
              <valList type="closed">
                <valItem ident="person"/>
                <valItem ident="place"/>
                <valItem ident="org"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
      </schemaSpec>
    </body>
  </text>
